{
    "image": "opencodeco/phpctl:php83-devcontainer",
    "features": {
      "ghcr.io/devcontainers/features/docker-in-docker:2": {},
      "ghcr.io/dapr/cli/dapr-cli:0": {}
    },
    "hostRequirements": {
      "cpus": 4
    },
    "customizations": {
      "vscode": {
          "extensions": [
            "phpactor.vscode-phpactor",
            "ms-azuretools.vscode-dapr"
        ]
      }
    },
    "portsAttributes": {
      "8080": {
        "label": "Application"
      }
    },
    "forwardPorts": [8080]
}
